<?xml version="1.0" encoding="utf-8"?>
<yanbuffer xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="xsd/yanbuffer.xsd">
	<!-- debug: true=send debug system messages to gms, false=don't send debug messages -->
	<!-- heal_cooldown: minimum seconds between heal usage -->
	<!-- max_unique_list: number of maximum unique bufflists per player -->
	<!-- unique_max_buffs: maximum number of BUFFS on a players unique bufflist -->
	<!-- unique_max_songs_dances: maximum number of SONGS_DANCES on a players unique bufflist -->
	<debug>false</debug>
	<heal_cooldown>60</heal_cooldown>
	<max_unique_lists>5</max_unique_lists>
	<unique_max_buffs>24</unique_max_buffs>
	<unique_max_songs_dances>12</unique_max_songs_dances>
</yanbuffer>